{
  "name": "namesake-fyi",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "dev": "astro dev --open",
    "start": "astro dev",
    "build": "wrangler types && astro check && astro build",
    "build:only": "astro build",
    "preview": "wrangler types && astro preview",
    "astro": "astro",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "format": "biome format",
    "format:fix": "biome format --write",
    "check": "astro check && biome check",
    "check:fix": "astro check && biome check --write",
    "test": "vitest",
    "test:once": "vitest run",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --reporter=html",
    "check-links": "lychee --config .lychee.toml .",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "lint-staged": {
    "*": "pnpm check:fix"
  },
