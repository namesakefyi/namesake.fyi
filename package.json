    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@axe-core/playwright": "^4.11.0",
    "@portabletext/types": "^3.0.0",
    "@remixicon/react": "^4.7.0",
    "@sanity/astro": "^3.2.10",
    "@sanity/client": "^7.13.0",
