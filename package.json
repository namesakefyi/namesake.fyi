    "@sanity/image-url": "^1.2.0",
    "@sanity/types": "^4.18.0",
    "@sanity/ui": "^3.1.11",
    "@severo_bo/us-atlas-2023": "^4.0.0",
    "astro": "5.16.0",
    "astro-embed": "^0.9.2",
    "astro-font": "^1.1.0",
    "astro-github-file-loader": "^1.1.0",
    "astro-portabletext": "^0.11.4",
    "d3": "^7.9.0",
